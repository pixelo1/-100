
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		//더하기 a,b
		int numA,numB;
    //반복횟수
		int t = sc.nextInt();
		
		for(int i = 1; i<= t; i++) {
			numA = sc.nextInt();
			numB = sc.nextInt();
			
			System.out.println("Case #" + i + ": " + numA + " + " + numB +" = " + (numA+numB));
		}

	}

}
